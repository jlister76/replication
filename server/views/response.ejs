<!DOCTYPE html>
<html lang="en" ng-app="Replication">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Locate Replications</title>
  <link rel="stylesheet" href="http://ajax.googleapis.com/ajax/libs/angular_material/1.1.0/angular-material.min.css">
  <style>
    section {
      width: 300px;
      height: 300px;

      position: absolute;
      top:0;
      bottom: 0;
      left: 0;
      right: 0;

      margin: 2rem auto;
    }




  </style>
</head>
<body>

<section>
  <h1><%= title %></h1>

  <% if (typeof content !== 'undefined') { %>

  <p><%= content %></p>

  <% } %>

  <a href="<%= redirectTo %>"><%= redirectToLinkText %></a>
</section>

</body>
</html>


